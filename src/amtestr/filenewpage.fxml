<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.collections.ObservableList?>
<?import java.lang.String?>
<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="amtestr.FileNewPageController">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <VBox alignment="CENTER" spacing="20.0">
               <children>
                  <Label alignment="CENTER_RIGHT" text="Име на теста:" />
                  <Label text="Автор на теста:" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" spacing="10.0">
               <children>
                  <TextField prefWidth="300.0" fx:id="testNameField"/>
                  <TextField maxWidth="200.0" prefWidth="200.0" fx:id="testAuthorField"/>
               </children>
            </VBox>
         </children></HBox>
      <ComboBox fx:id="testType" prefWidth="150.0" promptText="Вид на теста"/>
      <Button mnemonicParsing="false" text="Към въпросите" onAction="#toQuestions" />
   </children>
</VBox>
